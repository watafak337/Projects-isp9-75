#include <iostream>
using namespace std;
int main() {
    setlocale(0, "");
    int month;
    int day1;
    int day2;
    int day3;
    int day4;
    int day5;
    int day6;
    int day7;
    int day8;
    int day9;
    int day10;
    int day11;
    int day12;
    cout << "Программа: Календарь" << endl;

    cout << "[1] Январь" << endl;
    cout << "[2] Февраль" << endl;
    cout << "[3] Март" << endl;
    cout << "[4] Апрель" << endl;
    cout << "[5] Май" << endl;
    cout << "[6] Июнь" << endl;
    cout << "[7] Июль" << endl;
    cout << "[8] Август" << endl;
    cout << "[9] Сентябрь" << endl;
    cout << "[10] Октябрь" << endl;
    cout << "[11] Ноябрь" << endl;
    cout << "[12] Декабрь" << endl;

    cin >> month;

    switch (month) {
    case 1: {
        cout << "Выбран месяц - Январь" << endl;
        cout << "Выберите день месяца: ";
        cin >> day1;
        if (day1 >= 1 && day1 <= 31) {
            cout << "Календарь: " << day1 << " Января";
        }
        else {
            cout << "Ошибка! Вы ввели день не из этого месяца";
        }
        break;
        }
    case 2: {
        cout << "Выбран месяц - Феварль" << endl;
        cout << "Выберите день месяца: ";
        cin >> day1;
        if (day1 >= 1 && day1 <= 28) {
            cout << "Календарь: " << day1 << " Февараля";
        }
        else {
            cout << "Ошибка! Вы ввели день не из этого месяца";
        }
        break;
        }
    case 3: {
        cout << "Выбран месяц - Март" << endl;
        cout << "Выберите день месяца: ";
        cin >> day1;
        if (day1 >= 1 && day1 <= 31) {
            cout << "Календарь: " << day1 << " Марта";
        }
        else {
            cout << "Ошибка! Вы ввели день не из этого месяца";
        }
        break;
        }
    case 4: {
        cout << "Выбран месяц - Апрель" << endl;
        cout << "Выберите день месяца: ";
        cin >> day1;
        if (day1 >= 1 && day1 <= 30) {
            cout << "Календарь: " << day1 << " Апреля";
        }
        else {
            cout << "Ошибка! Вы ввели день не из этого месяца";
        }
        break;
        }
    case 5: {
        cout << "Выбран месяц - Май" << endl;
        cout << "Выберите день месяца: ";
        cin >> day1;
        if (day1 >= 1 && day1 <= 31) {
            cout << "Календарь: " << day1 << " Мая";
        }
        else {
            cout << "Ошибка! Вы ввели день не из этого месяца";
        }
        break;
        }\
    case 6: {
        cout << "Выбран месяц - Июнь" << endl;
        cout << "Выберите день месяца: ";
        cin >> day1;
        if (day1 >= 1 && day1 <= 30) {
            cout << "Календарь: " << day1 << " Июня";
        }
        else {
            cout << "Ошибка! Вы ввели день не из этого месяца";
        }
        break;
        }
    case 7: {
        cout << "Выбран месяц - Июль" << endl;
        cout << "Выберите день месяца: ";
        cin >> day1;
        if (day1 >= 1 && day1 <= 31) {
            cout << "Календарь: " << day1 << " Июля";
        }
        else {
            cout << "Ошибка! Вы ввели день не из этого месяца";
        }
        break;
        }
    case 8: {
        cout << "Выбран месяц - Август" << endl;
        cout << "Выберите день месяца: ";
        cin >> day1;
        if (day1 >= 1 && day1 <= 31) {
            cout << "Календарь: " << day1 << " Августа";
        }
        else {
            cout << "Ошибка! Вы ввели день не из этого месяца";
        }
        break;
        }
    case 9: {
        cout << "Выбран месяц - Сентябрь" << endl;
        cout << "Выберите день месяца: ";
        cin >> day1;
        if (day1 >= 1 && day1 <= 30) {
            cout << "Календарь: " << day1 << " Сентября";
        }
        else {
            cout << "Ошибка! Вы ввели день не из этого месяца";
        }
        break;
        }
    case 10: {
        cout << "Выбран месяц - Октябрь" << endl;
        cout << "Выберите день месяца: ";
        cin >> day1;
        if (day1 >= 1 && day1 <= 31) {
            cout << "Календарь: " << day1 << " Октября";
        }
        else {
            cout << "Ошибка! Вы ввели день не из этого месяца";
        }
        break;
        }
    case 11: {
        cout << "Выбран месяц - Ноябрь" << endl;
        cout << "Выберите день месяца: ";
        cin >> day1;
        if (day1 >= 1 && day1 <= 30) {
            cout << "Календарь: " << day1 << " Ноября";
        }
        else {
            cout << "Ошибка! Вы ввели день не из этого месяца";
        }
        break;
        }
    case 12: {
        cout << "Выбран месяц - Декабрь" << endl;
        cout << "Выберите день месяца: ";
        cin >> day1;
        if (day1 >= 1 && day1 <= 31) {
            cout << "Календарь: " << day1 << " Декабря";
        }
        else {
            cout << "Ошибка! Вы ввели день не из этого месяца";
        }
        break;
        }
        default:
            cout << "Такого месяца не существует" << endl;
            break;
        }
}
